      <instance>
        <census_household id="census_household" version="20200121">
          <deviceid/>
          <start/>
          <end/>
          <field_worker_id/>
          <visit_date/>
          <region_id/>
          <region_name/>
          <household_id/>
          <household_name/>
          <head_code/>
          <head_name/>
          <gps/>
          <location_processed>0</location_processed>
          <socialgroup_processed>0</socialgroup_processed>
          <meta>
            <instanceID/>
            <instanceName/>
          </meta>
        </census_household>
      </instance>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/census_household/deviceid" required="true()" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/census_household/start" required="true()" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/census_household/end" required="true()" type="dateTime"/>
      <bind nodeset="/census_household/field_worker_id" readonly="true()" required="true()" type="string"/>
      <bind constraint=". &lt;= today()" jr:constraintMsg="jr:itext('/census_household/visit_date:jr:constraintMsg')" nodeset="/census_household/visit_date" required="true()" type="date"/>
      <bind nodeset="/census_household/region_id" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/census_household/region_name" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/census_household/household_id" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/census_household/household_name" required="true()" type="string"/>
      <bind nodeset="/census_household/head_code" readonly="true()" type="string"/>
      <bind nodeset="/census_household/head_name" readonly="true()" type="string"/>
      <bind nodeset="/census_household/gps" required="true()" type="geopoint"/>
      <bind calculate="0" nodeset="/census_household/location_processed" readonly="true()" type="int"/>
      <bind calculate="0" nodeset="/census_household/socialgroup_processed" readonly="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/census_household/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="concat(&quot;household_&quot;,  /census_household/household_id )" nodeset="/census_household/meta/instanceName" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/census_household/field_worker_id">
      <label ref="jr:itext('/census_household/field_worker_id:label')"/>
    </input>
    <input appearance="no-calendar" ref="/census_household/visit_date">
      <label ref="jr:itext('/census_household/visit_date:label')"/>
    </input>
    <input ref="/census_household/region_id">
      <label ref="jr:itext('/census_household/region_id:label')"/>
    </input>
    <input ref="/census_household/region_name">
