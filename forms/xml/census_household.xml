    </input>
    <input ref="/census_household/head_name">
      <label ref="jr:itext('/census_household/head_name:label')"/>
    </input>
    <input ref="/census_household/gps">
      <label ref="jr:itext('/census_household/gps:label')"/>
      <hint>GPS coordinates can only be collected when outside.</hint>
    </input>
    <input ref="/census_household/location_processed">
      <label ref="jr:itext('/census_household/location_processed:label')"/>
    </input>
    <input ref="/census_household/socialgroup_processed">
      <label ref="jr:itext('/census_household/socialgroup_processed:label')"/>
    </input>
  </h:body>
</h:html>
